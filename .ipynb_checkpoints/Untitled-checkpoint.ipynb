    "#Ultrarelativistic Travel vs FTL\n",
    "\n",
    "def distance_newton(a,t):\n",
    "    t_sec = t*3.154*10**7\n",
    "    a_ms = a * 9.80665\n",
    "    \n",
    "    d_metre = 0.5*a_ms*t_sec**2\n",
    "    \n",
    "    d_ly = d_metre/(9.461*10**15)\n",
    "    return d_ly"
   ]
  }
